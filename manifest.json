{
  "name": "yebiwe",
  "version": "11",
  "description": "yebiwe",
  "content_scripts":[{
    "matches": ["<all_urls>"], 
    "js":["content.js"],
    "run_at": "document_end",
    "all_frames": false
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"

  },
  "manifest_version": 2
  
}
